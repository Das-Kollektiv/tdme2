ext-init:
	mkdir $(OBJ)

ext-compile: $(EXT_SRCS)
	cl /Fo$(OBJ)/ /c $(FLAGS) $(INCLUDES) $**

ext-link1: $(OBJ)/*.obj
	lib $(LD_FLAGS) /OUT:libtdme-ext.lib $**
	dumpbin /LINKERMEMBER libtdme-ext.lib > libtdme-ext.def.dumpbin.linkermember
	.\ext\windows-msc\tools\msclib2dll.exe libtdme-ext.def.dumpbin.linkermember
	del libtdme-ext.lib

ext-link2: $(OBJ)/*.obj
	.\ext\windows-msc\tools\createrc.exe resources/platforms/win32/lib.rc libtdme-ext.dll libtdme-ext.dll.rc
	rc libtdme-ext.dll.rc
	echo LIBRARY libtdme-ext.dll > libtdme-ext.def
	type libtdme-ext.def.dumpbin.linkermember.def >> libtdme-ext.def
	link $(LD_FLAGS) /DLL /DEF:libtdme-ext.def /IGNORE:4197 /IGNORE:4102 $** libtdme-ext.dll.res $(LIBS_LDFLAGS) /OUT:libtdme-ext.dll
	del libtdme-ext.def.dumpbin.linkermember
	del libtdme-ext.def.dumpbin.linkermember.def
	del libtdme-ext.dll.rc
	del libtdme-ext.dll.res

