init-bin:
	mkdir $(BIN)

ext-init:
	mkdir $(OBJ)

ext-compile: $(EXT_SRCS)
	cl /Fo$(OBJ)/ /c $(FLAGS) $(INCLUDES) $**

ext-link1: $(OBJ)/*.obj
	lib $(LD_FLAGS) /OUT:$(BIN)\libtdme-ext.lib $**
	dumpbin /LINKERMEMBER $(BIN)\libtdme-ext.lib > $(BIN)\libtdme-ext.def.dumpbin.linkermember
	.\ext\windows-msc\tools\msclib2dll.exe $(BIN)\libtdme-ext.def.dumpbin.linkermember
	del $(BIN)\libtdme-ext.lib

ext-link2: $(OBJ)/*.obj
	.\ext\windows-msc\tools\createrc.exe resources/platforms/win32/lib.rc $(BIN)\libtdme-ext.dll $(BIN)\libtdme-ext.dll.rc
	rc $(BIN)\libtdme-ext.dll.rc
	echo LIBRARY $(BIN)\libtdme-ext.dll > $(BIN)\libtdme-ext.def
	type $(BIN)\libtdme-ext.def.dumpbin.linkermember.def >> $(BIN)\libtdme-ext.def
	link $(LD_FLAGS) /DLL /DEF:$(BIN)\libtdme-ext.def /IGNORE:4197 /IGNORE:4102 $** $(BIN)\libtdme-ext.dll.res $(LIBS_LDFLAGS) /OUT:$(BIN)\libtdme-ext.dll

ext-clean:$(OBJ)/*.obj
	del /S /Q $(OBJ) >nul 2>&1
	rmdir $(OBJ)

