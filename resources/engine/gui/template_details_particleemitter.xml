						<layout alignment="horizontal" width="50%%" height="auto" horizontal-align="right" vertical-align="center">
							<text font="{$font.default}" color="{$color.font_normal}" text="max" width="auto" height="auto" />
							<space width="10" />	
							<input id="particleemitter_point_mass_max" width="*" height="auto" text="100" unit="kg" options="type=float,min=0,step=1,decimals=3" image="resources/engine/images/inputslider.png" />
						</layout>
					</layout>					
						
				</layout>
				<!-- PointParticleEmitter End -->
				
