<element id="{$id}" width="100%" height="auto">
	<layout width="100%" height="auto" padding="5" alignment="vertical" vertical-align="center" horizontal-align="center">
		<button width="100%" height="25" text="Particle Emitter" on-mouse-click="{$id}.condition!=open"/>
		<layout show-on="open" width="100%" height="auto" padding="5" alignment="vertical" vertical-align="top" horizontal-align="left">			
	
			<dropdown id="particleemitter_type" width="100%" height="auto" text="Select Emitter">
				<dropdown-option text="Point Emitter" value="1" on-mouse-click="particleemitter_type_details.condition=point" />
				<dropdown-option text="Box Emitter" value="2" on-mouse-click="particleemitter_type_details.condition=box" />
				<dropdown-option text="Circle Emitter" value="3" on-mouse-click="particleemitter_type_details.condition=circle" />
				<dropdown-option text="Radial Emitter" value="4" on-mouse-click="particleemitter_type_details.condition=radial" />
				<dropdown-option text="Sphere Emitter" value="5" on-mouse-click="particleemitter_type_details.condition=sphere" />
			</dropdown>
			
			<element id="particleemitter_type_details" width="100%" height="auto">
			
				<!-- PointParticleEmitter Start -->
				<layout show-on="point" alignment="vertical" width="100%" height="auto" horizontal-align="center" >
					<space height="5" />
					
					<layout width="100%" height="auto" alignment="horizontal" vertical-align="center">
						<text font="{$font.default}" color="{$color.font_normal}" text="Location" width="*" height="auto" horizontal-align="left" />
						<layout alignment="horizontal" width="50%" height="auto" horizontal-align="right">
							<layout width="5" height="25" background-color="{$color.xaxis}" />
							<input id="particleemitter_point_location_x" width="*" height="auto" text="0.0" unit="m" options="type=float,step=1,decimals=2" image="resources/engine/images/inputslider.png" />
						</layout>
					</layout>
					<space height="5" />
					<layout width="100%" height="auto" alignment="horizontal" vertical-align="center">
						<text font="{$font.default}" color="{$color.font_normal}" text="" width="*" height="auto" horizontal-align="left" />
						<layout alignment="horizontal" width="50%" height="auto" horizontal-align="right">
							<layout width="5" height="25" background-color="{$color.yaxis}" />
							<input id="particleemitter_point_location_y"  width="*" height="auto" text="0.0" unit="m" options="type=float,step=1,decimals=2" image="resources/engine/images/inputslider.png" />
						</layout>
					</layout>
					<space height="5" />
					<layout width="100%" height="auto" alignment="horizontal" vertical-align="center">
						<text font="{$font.default}" color="{$color.font_normal}" text="" width="*" height="auto" horizontal-align="left" />
						<layout alignment="horizontal" width="50%" height="auto" horizontal-align="right">
							<layout width="5" height="25" background-color="{$color.zaxis}" />
							<input id="particleemitter_point_location_z"  width="*" height="auto" text="0.0" unit="m" options="type=float,step=1,decimals=2" image="resources/engine/images/inputslider.png" />
						</layout>
					</layout>
					
					<space height="5" />
					<menu-separator />
					<space height="5" />
					
					<layout width="100%" height="auto" alignment="horizontal" vertical-align="center">
						<text font="{$font.default}" color="{$color.font_normal}" text="Color Start" width="*" height="auto"/>
						<layout alignment="horizontal" width="50%" height="auto" horizontal-align="right">
							<image id="particleemitter_point_colorstart" width="*" height="auto" src="resources/engine/images/button.png" scale9="10" effect-color-mul="red"/>
							<space width="5" />						
							<button id="button_particleemitter_point_colorstart" width="25" height="25" image="resources/engine/images/edit.png"/>
						</layout>
					</layout>
					
					<space height="5" />
					<layout width="100%" height="auto" alignment="horizontal" vertical-align="center">
						<text font="{$font.default}" color="{$color.font_normal}" text="Color End" width="*" height="auto"/>
						<layout alignment="horizontal" width="50%" height="auto" horizontal-align="right">
							<image id="particleemitter_point_colorend" width="*" height="auto" src="resources/engine/images/button.png" scale9="10" effect-color-mul="red"/>
							<space width="5" />						
							<button id="button_particleemitter_point_colorend" width="25" height="25" image="resources/engine/images/edit.png"/>
						</layout>
					</layout>
					
					<space height="5" />
					<menu-separator />
					<space height="5" />
					
					<layout width="100%" height="auto" alignment="horizontal" vertical-align="center">
						<text font="{$font.default}" color="{$color.font_normal}" text="Count" width="*" height="auto"/>
						<layout alignment="horizontal" width="50%" height="auto" horizontal-align="right">
							<input id="particleemitter_point_count" width="100%" height="auto" text="100" options="type=int,min=1,step=1" image="resources/engine/images/inputslider.png"/>
						</layout>
					</layout>
					
					<space height="5" />
					<menu-separator />
					<space height="5" />
					
					<layout width="100%" height="auto" alignment="horizontal" vertical-align="center">
						<text font="{$font.default}" color="{$color.font_normal}" text="Life Time" width="*" height="auto" />
						<layout alignment="horizontal" width="50%" height="auto" horizontal-align="right" vertical-align="center">
							<text font="{$font.default}" color="{$color.font_normal}" text="min" width="auto" height="auto" />
							<space width="10" />	
							<input id="particleemitter_point_lifetime_min" width="*" height="auto" text="100" unit="ms" options="type=int,min=0,step=1" image="resources/engine/images/inputslider.png" />>
						</layout>
					</layout>

					<space height="5" />

					<layout width="100%" height="auto" alignment="horizontal" vertical-align="center">
						<text font="{$font.default}" color="{$color.font_normal}" text="" width="*" height="auto" />
						<layout alignment="horizontal" width="50%%" height="auto" horizontal-align="right" vertical-align="center">
							<text font="{$font.default}" color="{$color.font_normal}" text="max" width="auto" height="auto" />
							<space width="10" />	
							<input id="particleemitter_point_lifetime_max" width="*" height="auto" text="100" unit="ms" options="type=int,min=0,step=1" image="resources/engine/images/inputslider.png" />
						</layout>
					</layout>					
					
					<space height="5" />
					<menu-separator />
					<space height="5" />
					
					<layout width="100%" height="auto" alignment="horizontal" vertical-align="center">
						<text font="{$font.default}" color="{$color.font_normal}" text="Velocity" width="*" height="auto" />
						<layout alignment="horizontal" width="50%" height="auto" horizontal-align="right" vertical-align="center">
							<text font="{$font.default}" color="{$color.font_normal}" text="min" width="auto" height="auto" />
							<space width="10" />	
							<input id="particleemitter_point_velocity_min" width="*" height="auto" text="100" unit="m/s" options="type=float,min=0,step=1,decimals=2" image="resources/engine/images/inputslider.png" />>
						</layout>
					</layout>

					<space height="5" />

					<layout width="100%" height="auto" alignment="horizontal" vertical-align="center">
						<text font="{$font.default}" color="{$color.font_normal}" text="" width="*" height="auto" />
						<layout alignment="horizontal" width="50%%" height="auto" horizontal-align="right" vertical-align="center">
							<text font="{$font.default}" color="{$color.font_normal}" text="max" width="auto" height="auto" />
							<space width="10" />	
							<input id="particleemitter_point_velocity_max" width="*" height="auto" text="100" unit="m/s" options="type=float,min=0,step=1,decimals=2" image="resources/engine/images/inputslider.png" />
						</layout>
					</layout>

					<space height="5" />
					<menu-separator />
					<space height="5" />					
					
					<layout width="100%" height="auto" alignment="horizontal" vertical-align="center">
						<text font="{$font.default}" color="{$color.font_normal}" text="Mass" width="*" height="auto" />
						<layout alignment="horizontal" width="50%" height="auto" horizontal-align="right" vertical-align="center">
							<text font="{$font.default}" color="{$color.font_normal}" text="min" width="auto" height="auto" />
							<space width="10" />	
							<input id="particleemitter_point_mass_min" width="*" height="auto" text="100" unit="kg" options="type=float,min=0,step=1,decimals=3" image="resources/engine/images/inputslider.png" />>
						</layout>
					</layout>

					<space height="5" />

					<layout width="100%" height="auto" alignment="horizontal" vertical-align="center">
