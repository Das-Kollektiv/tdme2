					<space height="5" />
					<template src="resources/engine/gui/template_details_shared_color.xml" id="particleproperties_colorend" name="Color     End" />				
					
					<space height="5" />
					<menu-separator />
					<space height="5" />
					
					<layout  width="100%" height="auto" alignment="horizontal" vertical-align="center">
