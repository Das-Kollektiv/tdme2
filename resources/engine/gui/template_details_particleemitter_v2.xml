						<layout alignment="horizontal" width="75%" height="auto" horizontal-align="right" vertical-align="center">
							<text font="{$font.default}" size="{$fontsize.default}" color="{$color.font_normal}" text="Start" width="auto" height="auto" />
							<space width="5" />
