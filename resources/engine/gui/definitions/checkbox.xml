<element id="{$id}" name="{$name}" value="{$value}" selected="{$selected}" disabled="{$disabled}" focusable="true" on-change="{$on-change}" on-mouse-click="{$on-mouse-click}" on-mouse-doubleclick="{$on-mouse-doubleclick}">
	<!-- unchecked, no mouse over -->
	<image effect-color-mul="#F0F0F0" hide-on="disabled,focus,checked,mouseover" src="resources/engine/images/checkbox_background.png" />
	<image effect-color-mul="#000000" hide-on="disabled,focus,checked,mouseover" src="resources/engine/images/checkbox_border.png" />
	<!-- unchecked, mouse over -->
	<image effect-color-mul="#B0B0FF" hide-on="disabled,focus,checked" show-on="mouseover" src="resources/engine/images/checkbox_background.png" />
	<image effect-color-mul="#000000" hide-on="disabled,focus,checked" show-on="mouseover" src="resources/engine/images/checkbox_border.png" />
	<!-- checked, no mouse over -->
	<image effect-color-mul="#8080FF" hide-on="disabled,focus,unchecked,mouseover" src="resources/engine/images/checkbox_background.png" />
	<image effect-color-mul="#000000" hide-on="disabled,focus,unchecked,mouseover" src="resources/engine/images/checkbox_border.png" />
	<image effect-color-mul="#FFFFFF" hide-on="disabled,focus,unchecked,mouseover" src="resources/engine/images/checkbox_mark.png" />
	<!-- checked, mouse over -->
	<image effect-color-mul="#B0B0FF" hide-on="disabled,focus,unchecked" show-on="mouseover" src="resources/engine/images/checkbox_background.png" />
	<image effect-color-mul="#000000" hide-on="disabled,focus,unchecked" show-on="mouseover" src="resources/engine/images/checkbox_border.png" />
	<image effect-color-mul="#FFFFFF" hide-on="disabled,focus,unchecked" show-on="mouseover" src="resources/engine/images/checkbox_mark.png" />
	<!-- unchecked, focus -->
	<image effect-color-mul="#B0B0FF" hide-on="disabled,checked" show-on="focus" src="resources/engine/images/checkbox_background.png" />
	<image effect-color-mul="#8080FF" hide-on="disabled,checked" show-on="focus" src="resources/engine/images/checkbox_border.png" />
	<!-- checked, focus -->
	<image effect-color-mul="#B0B0FF" hide-on="disabled,unchecked" show-on="focus" src="resources/engine/images/checkbox_background.png" />
	<image effect-color-mul="#8080FF" hide-on="disabled,unchecked" show-on="focus" src="resources/engine/images/checkbox_border.png" />
	<image effect-color-mul="#FFFFFF" hide-on="disabled,unchecked" show-on="focus" src="resources/engine/images/checkbox_mark.png" />
	<!-- unchecked, disabled -->
	<image effect-color-mul="#A0A0A0" hide-on="checked" show-on="disabled" src="resources/engine/images/checkbox_background.png" />	
	<image effect-color-mul="#A0A0A0" hide-on="checked" show-on="disabled" src="resources/engine/images/checkbox_border.png" />
	<!-- checked, disabled -->
	<image effect-color-mul="#A0A0A0" hide-on="unchecked" show-on="disabled" src="resources/engine/images/checkbox_background.png" />
	<image effect-color-mul="#A0A0A0" hide-on="unchecked" show-on="disabled" src="resources/engine/images/checkbox_border.png" />
	<image effect-color-mul="#FFFFFF" hide-on="unchecked" show-on="disabled" src="resources/engine/images/checkbox_mark.png" />
</element>
