	$i = int(1)
	forCondition(lesser($i, 5))
		console.log($i, ":")
		if (equals($i, 0))
			console.log("i -> 0")
		elseif (equals($i, 1))
			console.log("i -> 1")
		elseif (equals($i, 2))
			console.log("i -> 2")
		elseif (equals($i, 3))
			console.log("i -> 3")
		else
			console.log("else: ", $i)
		end
