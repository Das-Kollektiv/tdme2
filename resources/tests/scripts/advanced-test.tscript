# initialize
on: initialize
	console.log("---------------------------")
	console.log("advanced-test: Initialize")
	console.log("--------------------------")
	console.log()
end

# if no condition is met, nothing will be executed, lol :D
on: nothing
	# array
	$array = [1, 2, 3, [4, 5, [6, 7, 8, "What?", "Whatssssappp???", 9]]]
	console.log("array: " + $array)

	$i = 0
	forCondition($i < $array->length())
		console.log($i + ": " + $array[$i])
		++$i
	end

	# map
	$map = {
		test1: "test1",
		test2: "test2",
		test3: 789
	}
	console.log("map: " + $map)
	console.log("map keys: " + $map->getKeys())
	console.log("map values: " + $map->getValues())

	# done
	script.stop()
end

# an error has occurred
on: error
	console.log("----------------------")
	console.log("advanced-test: Error")
	console.log("----------------------")
	console.log("An error occurred")
	script.wait(1000)
end
